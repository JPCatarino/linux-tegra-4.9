	host1x {
		//delete-node/ vi@15c10000;
		vi_base: vi@15c10000  {
			status="okay";
			num-channels = <2>;
			ports {
				#address-cells = <1>;
				#size-cells = <0>;
				vi_port0: port@0 {
					status="okay";
					reg = <0>;
